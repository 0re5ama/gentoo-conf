server {
    server_name ip.valk.nand.wakku.to ip.valkyrja.xyz;
    listen [::]:80;
    listen [::]:443 ssl http2;
    listen 80;
    listen 443 ssl http2;

    location / {
        default_type text/plain;
        echo $remote_addr;
    }
}

